<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PlayableRecyclerView">
        <attr name="playable_player_pool_count" format="integer" />
        <attr name="playable_player_concurrent_max" format="integer" />
        <attr name="playable_type" format="enum">
            <enum name="EXOPLAYER" value="0" />
        </attr>
        <attr name="playable_autoplay" format="boolean" />
        <attr name="playable_loop_type" format="enum">
            <enum name="loop" value="0" />
            <enum name="none" value="1" />
        </attr>
        <attr name="playable_pause_during_invisible" format="boolean" />
        <attr name="playable_show_default_loading" format="boolean" />
        <attr name="playable_cache_max_size_mb" format="integer"/>
        <attr name="playable_use_cache" format="boolean"/>
    </declare-styleable>
</resources>